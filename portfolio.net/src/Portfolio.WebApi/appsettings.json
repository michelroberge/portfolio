{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",

    "Authentication": {
        "Google": {
            "Enabled": true,
            "ClientId": "YOUR_GOOGLE_CLIENT_ID",
            "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET"
        },
        "GitHub": {
            "Enabled": true,
            "ClientId": "YOUR_GITHUB_CLIENT_ID",
            "ClientSecret": "YOUR_GITHUB_CLIENT_SECRET"
        },
        "Local": {
            "Enabled": true
        }
    }
}

